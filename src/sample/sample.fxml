<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <Pane fx:id="isConnected" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="1080.0" prefWidth="1920.0">
               <children>
                  <TableView fx:id="descriptionTable" prefHeight="1080.0" prefWidth="1920.0">
                    <columns>
                        <TableColumn fx:id="sectorColumn" editable="false" minWidth="0.0" prefWidth="759.3999633789062" text="Sector" />
                        <TableColumn fx:id="suggestedCodesNaf" minWidth="0.0" prefWidth="259.0" text="suggested Codes | Confidence" />
                      <TableColumn fx:id="activitiesColumn" editable="false" prefWidth="655.4000121951103" text="Profession" />
                        <TableColumn fx:id="suggestedCodesPcs" minWidth="0.0" prefWidth="248.5999755859375" text="suggested Codes | Confidence" />
                    </columns>
                  </TableView>
                  <TextField fx:id="filterField" prefHeight="25.0" prefWidth="97.0" />
                  <Button fx:id="saveDescription" mnemonicParsing="false" onAction="#saveDescription" text="Save" />
               </children>
            </VBox>
         </children></Pane>
   </center>
</BorderPane>
