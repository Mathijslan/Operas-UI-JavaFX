<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#openThresholdMenu" text="Threshold" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <Pane fx:id="isConnected" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="1080.0" prefWidth="1920.0">
               <children>
                  <TableView fx:id="descriptionTable" prefHeight="909.0" prefWidth="1834.0">
                    <columns>
                        <TableColumn fx:id="sectorColumn" editable="false" minWidth="0.0" prefWidth="539.999981701374" text="Sector" />
                        <TableColumn fx:id="suggestedCodesNaf" minWidth="0.0" prefWidth="183.20001220703125" text="suggested Codes | Confidence" />
                        <TableColumn fx:id="nafManualColumn" minWidth="0.0" prefWidth="247.20001220703125" text="Manual code" />
                      <TableColumn fx:id="activitiesColumn" editable="false" prefWidth="541.9999389648438" text="Profession" />
                        <TableColumn fx:id="suggestedCodesPcs" minWidth="0.0" prefWidth="98.4000244140625" text="suggested Codes | Confidence" />
                        <TableColumn fx:id="pcsManualColumn" minWidth="0.0" prefWidth="98.4000244140625" text="Manual code" />
                    </columns>
                  </TableView>
                  <TextField fx:id="filterField" editable="false" opacity="0.0" prefHeight="26.0" prefWidth="913.0" />
                  <Button fx:id="saveDescription" mnemonicParsing="false" onAction="#saveDescription" text="Save" />
               </children>
            </VBox>
         </children></Pane>
   </center>
</BorderPane>
